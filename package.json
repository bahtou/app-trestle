{
  "name": "app-trestle",
  "version": "1.0.0",
  "description": "App Trestle is the barebones of a foundation to quickly start building your web application. Webpack serves as the trestle that supports your application's disparate technologies to give way to a rigid frame to which to build upon.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahtou/app-trestle.git"
  },
  "keywords": [
    "trestle",
    "webpack",
    "foundation",
    "bundler"
  ],
  "author": "bahtou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahtou/app-trestle/issues"
  },
  "homepage": "https://github.com/bahtou/app-trestle#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs3": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "error-overlay-webpack-plugin": "^0.2.0",
    "eslint": "^5.15.3",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "extract-css-chunks-webpack-plugin": "^4.2.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-url": "^8.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@hot-loader/react-dom": "^16.8.6",
    "core-js": "^3.0.0",
    "react": "^16.8.6",
    "react-hot-loader": "^4.8.2"
  },
  "scripts": {
    "build": "webpack --config ./webpack/index.js --env production",
    "build:dev": "webpack --config ./webpack/index.js --env production && python3 -m http.server 8000 --directory build",
    "refresh": "rm -rf node_modules/ package-lock.json && npm install",
    "dev": "nodemon --watch webpack --exec 'webpack-dev-server --config ./webpack/index.js --env development'"
  }
}
